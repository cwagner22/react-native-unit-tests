  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-react-native": "^4.0.1",
    "chai": "3.5.0",
    "enzyme": "2.7.1",
    "jest": "^24.1.0",
    "mocha": "3.2.0",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "15.4.2",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/jest/*.js?(x)"
